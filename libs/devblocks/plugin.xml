		<extension point="devblocks.storage.engine">
			<id>devblocks.storage.engine.disk</id>
			<name>Disk</name>
			<class>
				<file>api/services/storage.php</file>
				<name>DevblocksStorageEngineDisk</name>
			</class>
			<params>
			</params>
