		</file>
		<file path="api/services/data/ui_icons.php">
			<class name="_DevblocksDataProviderUiIcons" />
		</file>
		<file path="api/services/data/usage_bot_behaviors.php">
			<class name="_DevblocksDataProviderUsageBotBehaviors" />
