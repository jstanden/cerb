<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://www.devblocks.com/schema/plugin.xsd">
	<id>example.ticket.toolbaritem</id>
	<name>[Examples] Example Ticket Toolbar Button</name>
	<description>This plugin adds an example button to the ticket toolbar</description>
	<author>WebGroup Media, LLC.</author>
	<version>0.0.0</version>
	<link>http://wiki.cerb5.com/wiki/Extension_Point:cerberusweb.ticket.toolbaritem</link>
	
	<!-- Plugin Dependencies -->
	
	<dependencies>
		<require plugin_id="cerberusweb.core" version="5.1.0-dev" />
	</dependencies>
	
	<!-- Exported Classes -->

	<!--	
	<class_loader>
		<file path="api/App.php">
		</file>
	</class_loader>
	-->
	
	<extensions>
    
    	<extension point="cerberusweb.ticket.toolbaritem">
			<id>example.ticket.toolbaritem.button</id>
			<name>Example Ticket Toolbar Button</name>
			<class>
				<file>api/App.php</file>
				<name>ExTicketToolbarItem</name>
			</class>
			<params>
			</params>
		</extension>
		
		
	</extensions>
</plugin>