{
  "package": {
    "name": "Connected Service: Smartsheet",
    "revision": 1,
    "requires": {
      "cerb_version": "9.2.0",
      "plugins": [

      ]
    },
    "library": {
      "name": "Smartsheet",
      "uri": "cerb_connected_service_smartsheet",
      "description": "Integration with Smartsheet",
      "instructions": "# Instructions\r\n\r\n* See [**Cerb + Smartsheet**](https://cerb.ai/guides/integrations/smartsheet/)\r\n* Enter the Access Token below",
      "point": "connected_service",
      "image": "data:image/png;base64,"
    },
    "configure": {
      "placeholders": [],
      "prompts": [
        {
          "type": "text",
          "label": "Access Token",
          "key": "prompt_access_token",
          "params": {
            "default": "",
            "placeholder": "(paste your Access Token)"
          }
        }
      ]
    }
  },
  "records": [
    {
      "uid": "service_smartsheet",
      "_context": "connected_service",
      "name": "Smartsheet",
      "extension_id": "cerb.service.provider.token.bearer",
      "params": {
        "token_name": "Bearer"
      }
    },
    {
      "uid": "account_smartsheet",
      "_context": "connected_account",
      "name": "Smartsheet",
      "service_id": "{{{uid.service_smartsheet}}}",
      "owner__context": "cerberusweb.contexts.app",
      "owner_id": "0",
      "params": {
        "token": "{{{prompt_access_token}}}"
      }
    }
  ]
}